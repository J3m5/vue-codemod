{
  "name": "@originjs/vue-codemod",
  "version": "1.1.1",
  "description": "Vue codemod scripts",
  "main": "dist/index.js",
  "bin": "./dist/bin/vue-codemod.js",
  "files": [
    "dist",
    "!dist/**/__tests__/**"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc --build --watch",
    "prepublishOnly": "tsc",
    "playground": "npm run build && cd ./playground && npm install && npm run dev",
    "test": "jest",
    "vitest": "vitest",
    "test:cover": "jest --coverage",
    "fix": "npm-run-all --parallel lint:fix format:fix",
    "lint:codes": "eslint . --ext .ts",
    "lint:fix": "npm run lint:codes --fix",
    "format:prettier": "prettier --config .prettierrc .",
    "format:fix": "prettier --write --config .prettierrc ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/originjs/vue-codemod.git"
  },
  "author": "ygj6",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.22.20",
    "@babel/preset-env": "^7.22.20",
    "@babel/types": "^7.22.19",
    "@types/jscodeshift": "^0.7.1",
    "@vue/compiler-core": "~3.0.5",
    "@vue/compiler-dom": "~3.0.5",
    "cli-progress": "^3.12.0",
    "debug": "^4.3.4",
    "fs-extra": "^11.1.1",
    "globby": "^11.0.2",
    "jscodeshift": "^0.11.0",
    "lodash": "^4.17.21",
    "lru-cache": "^10.0.1",
    "readline-sync": "^1.4.10",
    "recast": "^0.23.4",
    "source-map": "^0.7.4",
    "table": "^6.8.1",
    "vue-eslint-parser": "^9.3.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.2",
    "@types/debug": "^4.1.8",
    "@types/eslint-scope": "^3.7.4",
    "@types/eslint-visitor-keys": "^1.0.0",
    "@types/fs-extra": "^11.0.2",
    "@types/jest": "^29.5.5",
    "@types/lodash": "^4.14.198",
    "@types/node": "^18.17.17",
    "@types/readline-sync": "^1.4.4",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "@typescript-eslint/utils": "^6.7.2",
    "ast-types": "0.12.1",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "vitest": "^0.34.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "maxWorkers": "50%",
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/"
    ]
  },
  "engines": {
    "node": ">= 10.0"
  }
}
