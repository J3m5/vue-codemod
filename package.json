{
  "name": "vue-codemod",
  "version": "1.1.2",
  "description": "Vue codemod scripts",
  "main": "dist/index.js",
  "bin": "dist/bin/vue-codemod.js",
  "files": [
    "dist",
    "!dist/**/__tests__/**"
  ],
  "scripts": {
    "build": "tsc  -p tsconfig.build.json",
    "watch": "tsc --watch --noEmit",
    "build-watch": "tsc --watch",
    "prepublishOnly": "tsc",
    "playground": "npm run build && cd ./playground && npm install && npm run dev",
    "vitest": "vitest",
    "test:cover": "jest --coverage",
    "fix": "npm-run-all --parallel lint:fix format:fix",
    "lint:codes": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format:prettier": "prettier --config .prettierrc .",
    "format:fix": "prettier --write --config .prettierrc .",
    "postinstall": "pnpm run build",
    "install-global": "pnpm run build && pnpm link --global"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/originjs/vue-codemod.git"
  },
  "author": "ygj6",
  "license": "MIT",
  "dependencies": {
    "@vue/compiler-core": "^3.3.7",
    "@vue/compiler-dom": "^3.3.7",
    "cli-progress": "^3.12.0",
    "debug": "^4.3.4",
    "fs-extra": "^11.1.1",
    "globby": "^11.1.0",
    "jscodeshift": "^0.15.0",
    "lodash": "^4.17.21",
    "lru-cache": "^10.0.1",
    "readline-sync": "^1.4.10",
    "recast": "0.23.4",
    "source-map": "^0.7.4",
    "table": "^6.8.1",
    "vue-eslint-parser": "^9.3.2",
    "yargs": "^17.7.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.4",
    "@types/debug": "^4.1.10",
    "@types/eslint-scope": "^3.7.6",
    "@types/eslint-visitor-keys": "^3.3.0",
    "@types/fs-extra": "^11.0.3",
    "@types/jest": "^29.5.6",
    "@types/jscodeshift": "^0.11.9",
    "@types/lodash": "^4.14.200",
    "@types/node": "^20.8.9",
    "@types/readline-sync": "^1.4.6",
    "@types/yargs": "^17.0.29",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "ast-types": "0.16.1",
    "bun": "^1.0.7",
    "bun-types": "1.0.7",
    "eslint": "^8.52.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">= 18.0",
    "pnpm": ">= 8.0.0"
  },
  "pnpm": {
    "overrides": {
      "ast-types": "0.16.1",
      "recast": "0.23.4"
    },
    "patchedDependencies": {
      "ast-types@0.16.1": "patches/ast-types@0.16.1.patch"
    }
  }
}