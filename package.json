{
  "name": "@originjs/vue-codemod",
  "version": "1.1.1",
  "description": "Vue codemod scripts",
  "main": "dist/index.js",
  "bin": "./dist/bin/vue-codemod.js",
  "files": [
    "dist",
    "!dist/**/__tests__/**"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch --noEmit",
    "prepublishOnly": "tsc",
    "playground": "npm run build && cd ./playground && npm install && npm run dev",
    "vitest": "vitest",
    "test:cover": "jest --coverage",
    "fix": "npm-run-all --parallel lint:fix format:fix",
    "lint:codes": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format:prettier": "prettier --config .prettierrc .",
    "format:fix": "prettier --write --config .prettierrc ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/originjs/vue-codemod.git"
  },
  "author": "ygj6",
  "license": "MIT",
  "dependencies": {
    "@types/jscodeshift": "^0.11.7",
    "@vue/compiler-core": "~3.3.4",
    "@vue/compiler-dom": "~3.3.4",
    "cli-progress": "^3.12.0",
    "debug": "^4.3.4",
    "fs-extra": "^11.1.1",
    "globby": "^11.1.0",
    "jscodeshift": "^0.15.0",
    "lodash": "^4.17.21",
    "lru-cache": "^10.0.1",
    "readline-sync": "^1.4.10",
    "recast": "^0.23.4",
    "source-map": "^0.7.4",
    "table": "^6.8.1",
    "vue-eslint-parser": "^9.3.1",
    "yargs": "^17.7.2",
    "zod": "^3.22.3"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.3",
    "@types/debug": "^4.1.9",
    "@types/eslint-scope": "^3.7.5",
    "@types/eslint-visitor-keys": "^1.0.0",
    "@types/fs-extra": "^11.0.2",
    "@types/jest": "^29.5.5",
    "@types/lodash": "^4.14.199",
    "@types/node": "^18.18.3",
    "@types/readline-sync": "^1.4.5",
    "@types/yargs": "^17.0.26",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "ast-types": "0.16.1",
    "eslint": "^8.50.0",
    "npm-run-all": "^4.1.5",
    "patch-package": "^8.0.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">= 18.0"
  },
  "pnpm": {
    "overrides": {
      "ast-types": "0.16.1"
    }
  }
}
